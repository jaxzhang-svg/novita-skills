{
  "name": "twitterapi-skill",
  "version": "0.1.0",
  "description": "A CLI tool for TwitterAPI.io - streamlined API wrapper with field filtering",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "twitterapi": "./dist/cli.js",
    "tw": "./dist/cli.js"
  },
  "scripts": {
    "dev": "bun run src/cli.ts",
    "build": "bun build src/cli.ts --outfile ../scripts/twitterapi --target bun --compile && chmod +x ../scripts/twitterapi",
    "build:js": "tsc",
    "watch": "bun --watch src/cli.ts",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "test": "bun test"
  },
  "keywords": [
    "twitter",
    "api",
    "cli",
    "twitterapi"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/twitterapi-skill"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "dotenv": "^16.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "bun-types": "^1.0.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
